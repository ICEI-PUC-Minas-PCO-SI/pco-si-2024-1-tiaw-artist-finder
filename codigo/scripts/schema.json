{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "nome",
        "idade",
        "foto",
        "estado",
        "instituicao",
        "disponibilidade",
        "capa",
        "atuacao"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                1
            ]
        },
        "nome": {
            "type": "string",
            "default": "",
            "title": "The nome Schema",
            "examples": [
                "Ana Silva"
            ]
        },
        "idade": {
            "type": "integer",
            "default": 0,
            "title": "The idade Schema",
            "examples": [
                22
            ]
        },
        "foto": {
            "type": "string",
            "default": "",
            "title": "The foto Schema",
            "examples": [
                "assets/img/pessoa1.jpg"
            ]
        },
        "estado": {
            "type": "string",
            "default": "",
            "title": "The estado Schema",
            "examples": [
                "SP"
            ]
        },
        "instituicao": {
            "type": "string",
            "default": "",
            "title": "The instituicao Schema",
            "examples": [
                "PUC MINAS"
            ]
        },
        "disponibilidade": {
            "type": "string",
            "default": "",
            "title": "The disponibilidade Schema",
            "examples": [
                "Freelance"
            ]
        },
        "capa": {
            "type": "string",
            "default": "",
            "title": "The capa Schema",
            "examples": [
                "assets/img/capa1.jpg"
            ]
        },
        "atuacao": {
            "type": "string",
            "default": "",
            "title": "The atuacao Schema",
            "examples": [
                "Web Design"
            ]
        }
    },
    "examples": [{
        "id": 1,
        "nome": "Ana Silva",
        "idade": 22,
        "foto": "assets/img/pessoa1.jpg",
        "estado": "SP",
        "instituicao": "PUC MINAS",
        "disponibilidade": "Freelance",
        "capa": "assets/img/capa1.jpg",
        "atuacao": "Web Design"
    }]
}